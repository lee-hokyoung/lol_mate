            td 플래티넘
            td 시간당
            td
              input.form-control(type='text', name='platinum', value=tutorGameInfo.tutorGame.platinum)
          tr
            td 다이아몬드
            td 시간당
            td
              input.form-control(type='text', name='diamond', value=tutorGameInfo.tutorGame.diamond)
          tr
            td 언랭배치 + 골드보장 
            td 
