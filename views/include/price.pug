            td
              input.form-control(type='text', name='gold', value=rankGameInfo.rankGame.gold)
          tr
            td 플래티넘
            td 배치고사 강의 10판
            td
              input.form-control(type='text', name='platinum', value=rankGameInfo.rankGame.platinum)
          tr
            td 다이아몬드
            td 배치고사 강의 10판
            td
              input.form-control(type='text', name='diamond', value=rankGameInfo.rankGame.diamond)   
    .col-md-6
      .d-flex.justify-content-between
        h5.text-primary 1:1 전문강의
